<div class="windowOne">
  <div class="border border-primary" style="width: 75vw">
    <div class="custLeftPanel">
      <button
        id="viewCustomersButton"
        type="button"
        class="btn btn-outline-primary custButtons"
      >
        View Customers
      </button>
      <button id="" type="button" class="btn btn-outline-primary custButtons">
        Another button?
      </button>
    </div>

      <div class="customerInformation">
        {{#each renderedCustomers as |customer|}}
          <p>{{customer.id}}.
          <a href="./{{customer.id}}">View
            {{customer.first_name}}
            {{customer.last_name}}</a></p>
        {{/each}}
      </div>

  </div>

  <div class="border border-primary rightPanelStorage">
    <div class="canvasChartStorage">
      <canvas id="myChart"></canvas>
    </div>
  </div>

  </div>


<script>
  let myChart = document.getElementById("myChart").getContext("2d"); let
  massPopChart = new Chart(myChart, { type: "doughnut", data: { labels:
  ["Insured", "Uninsured", ], datasets: [ { label: "Customer Insurance", data: [{{insured}},
  {{uninsured}}], backgroundColor:['#1ee95b', '#ffee00'] }, ],}, options: {}, });

</script>